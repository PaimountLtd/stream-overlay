{
  "name": "@streamlabs/game-overlay",
  "private": true,
  "description": "Streamlabs Game Overlay module",
  "gypfile": true,
  "main": "npm/index.js",
  "typings": "npm/typings.d.ts",
  "author": {
    "name": "Streamlabs",
    "url": "https://streamlabs.com"
  },
  "version": "0.0.14",
  "license": "SEE LICENSE IN COPYRIGHT LAW",
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stream-labs/streamlabs-overlay"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --msvs_version=2017",
    "pack": "node scripts/pack.js",
    "build": "node-gyp configure && node-gyp rebuild --target=2.0.16 --dist-url=https://atom.io/download/atom-shell",
    "pack-gyp": "node-pre-gyp package",
    "release": "standard-version",
    "test": "node index.test.js"
  },
  "binary": {
    "module_name": "streamlabs_overlay",
    "module_path": "./build/{configuration}/{node_abi}-{napi_build_version}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{napi_build_version}-{platform}-{arch}.tar.gz",
    "host": "https://streamlabs-overlay-binary.s3-us-east-1.amazonaws.com",
    "napi_versions": [
      1
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf",
    "trailingComma": "all"
  },
  "files": [
    "npm/index.js",
    "npm/typings.d.ts",
    "npm/package.json",
    "build/*/*/streamlabs_overlay.node"
  ],
  "devDependencies": {
    "@types/node": "^11.10.4",
    "node-pre-gyp": "^0.12.0",
    "aws-sdk": "^2.x",
    "electron": "^2.0.16",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.1",
    "fs-extra": "7.0.1",
    "tar": "^4.4.8"
  },
  "dependencies": {
    
  }
}
